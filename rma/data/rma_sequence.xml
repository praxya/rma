<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="rma_seq" model="ir.sequence">
        <field name="name">RMA sequence</field>
        <field name="code">rma.order</field>
        <field name="padding">5</field>
        <field name="prefix">RMA/%(year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
    </record>

    <record id="rma_supplier_seq" model="ir.sequence">
        <field name="name">RMA supplier sequence</field>
        <field name="code">rma.order.supplier</field>
        <field name="padding">5</field>
        <field name="prefix">V/RMA/%(year)s/</field>
        <field eval="1" name="number_next"/>
        <field eval="1" name="number_increment"/>
    </record>
</odoo>
